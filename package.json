    "rescript"
  ],
  "dependencies": {
    "@rescript/react": "^0.10.3",
    "bs-css-emotion": "^4.0.1",
    "concurrently": "^6.2.1",
    "parcel": "^2.0.0-rc.0",
    "parcel-bundler": "^1.12.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reason-date-fns": "^1.0.2",
    "rescript": "*"
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-nightly.1981"
  }
}
